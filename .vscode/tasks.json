{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Catkin",
			"command": "catkin build -DCMAKE_BUILD_TYPE=Release -DCMAKE_C_COMPILER=/usr/bin/gcc-8",
			"group": "build"
		},
		{
			"type": "shell",
			"label": "launch",
			"group": "none",
			"command": "gnome-terminal --window -- roslaunch dvc dronevisualsim.launch  "
		},
		{
			"type": "shell",
			"label": "predator",
			"group": "none",
			"command": "./startpredator.sh"
		},
		{
			"type": "shell",
			"label": "prey",
			"group": "none",
			"command": "./startprey.sh"
		},
		{
			"type": "shell",
			"label": "lern",
			"group": "none",
			"command": "roslaunch dvc training_session.launch  "
		},
		{
			"type": "shell",
			"label": "Pilot",
			"group": "none",
			"command": "rosrun dvc pilot"
		}
	]
}